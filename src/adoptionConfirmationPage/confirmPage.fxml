<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="adoptionConfirmationPage.ConfirmPage">
  <children>
    <AnchorPane layoutX="-1.0" prefHeight="403.0" prefWidth="600.0">
      <children>
        <Button fx:id="btnReturntoList" layoutX="402.0" layoutY="102.0" mnemonicParsing="false"
          onAction="#goToAnimalList" text="Return to Animal List"/>
        <Button fx:id="btnReturntoMain" layoutX="398.0" layoutY="153.0" mnemonicParsing="false"
          onAction="#goToMain" text="Return to Main Screen"/>
        <Button fx:id="btnLogOff" layoutX="448.0" layoutY="202.0" mnemonicParsing="false"
          onAction="#LogUserOut" text="Log off"/>
        <TextArea fx:id="txtfieldThanks" layoutX="100.0" layoutY="69.0" prefHeight="144.0"
          prefWidth="200.0"
          text="Thank you for applying to adopt our wonderful friend! We will look through your application to ensure that your home is right for them!"
          wrapText="true"/>
      </children>
    </AnchorPane>
  </children>
</AnchorPane>
